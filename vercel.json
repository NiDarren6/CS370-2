{
  "version": 2,
  "builds": [
    {
      "src": "api/redshift_server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "package.json"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/redshift_server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
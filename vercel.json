{
    "version": 2,
    "builds": [
      {
        "src": "redshift_server.cjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "redshift_server.cjs"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
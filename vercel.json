{
"version": 2,
  "builds": [
    {
      "src": "api/redshift_server.cjs",
      "use": "@vercel/node"
    },
    {
        "src": "index.html",
        "use": "tsc && vite build"
      }

    
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/redshift_server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}



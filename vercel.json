{
    "version": 2,
    "builds": [
      {
        "src": "redshift_server.cjs",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "redshift_server.cjs"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
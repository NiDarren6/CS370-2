{
  "version": 2,
  "builds": [
    {
      "src": "redshift_server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "redshift_server.cjs"
    },
    {
      "src": "^/(?!api).*",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
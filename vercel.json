{
    "version": 2,
    "builds": [
      {
        "src": "redshift_server.cjs",
        "use": "@vercel/node"
      },
      {
        "src": "/",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "redshift_server.cjs"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites":[
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }